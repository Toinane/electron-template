{
    "name": "electron-template",
    "productName": "Electron Template",
    "version": "1.0.0",
    "description": "Electron Template is a Github Template for Electron",
    "repository": "git@github.com:Toinane/electron-template.git",
    "author": "toinane <toinane@crea-that.fr>",
    "license": "MIT",
    "main": "app/app.js",
    "scripts": {
        "start": "electron app/app.js",
        "release": "electron-builder --publish=never --config electron-builder.json",
        "release:linux": "electron-builder --publish=never --config electron-builder.json --linux deb, apk, rpm, zip, appImage",
        "release:window": "electron-builder --publish=never --config electron-builder.json --win nsis, portable"
    },
    "dependencies": {
        "electron-light-storage": "^1.1.0"
    },
    "devDependencies": {
        "electron": "^7.1.7",
        "electron-builder": "^21.2.0"
    }
}
